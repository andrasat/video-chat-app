{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"organizeImports": {
		"enabled": false
	},
	"files": {
		"ignore": [
			"**/dist/**/*",
			"**/node_modules/**/*",
			"**/test/**/*",
			"**/.yarn/**/*",
			".husky/install.mjs",
			".pnp.cjs",
			".pnp.loader.mjs"
		]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
